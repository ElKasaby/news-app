<body>
    {{#each data}}
    <div class="card" style="width: 18rem;">
        <img src="{{urlToImage}}" class="card-img-top" alt="Error image not found">
        <div class="card-body">
            <h5 class="card-title" >{{title}}</h5>
            <p class="card-text">{{description}}</p>
            <a href="{{url}}" class="btn btn-primary">Go to artical</a>
        </div>
    </div>
    {{/each}}

